<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- Disable cloud backup -->
    <cloud-backup disableIfNoEncryptionCapabilities="true">
        <exclude domain="*"/>
    </cloud-backup>

    <!-- Disable device-to-device transfer -->
    <device-transfer>
        <exclude domain="*"/>
    </device-transfer>
</data-extraction-rules>
